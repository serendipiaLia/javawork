package inheritance.q4;

public class Employee {

	public String name;
	public String grade;
	
	public Employee(String name) {
		this.name = name;
	}
}

	private String skilset;
	public String getSkillSet() {
		return skillset;
	}
	
	public void setSkillSet(String skillset) {
		this.skillSet = skillset;
	}
