package array2d;

public class Array2dApt {
	public static void main(String[] args) {
		// 아파트 세대 구현하기
		// 5층, 한 층당 - 3세대
		// 3층, 1세대
		int[][] household = new int[5][3]; //[행][열]
		
		System.out.println("2층 %d세대\n", household[1].length);
		System.out.println("1층 %d세대\n", household[0].length);
		
		
		
	}
}
