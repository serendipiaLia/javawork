package methods;

public class ReturnMethodTest {
	
	// return이 있는 함수(반환 자료형) - int, String
	
	
	public static void main(String[] args) {
		/*int n1 = 10, n2 = 20;
		int sum = n1 + n2;
		System.out.println(sum); */
		
		//더하기 함수 호출
		add(10, 20);
		
	}  //m
	
	public static int add(int a, int b) {
		return a + b;
		
	}
	
	
}
