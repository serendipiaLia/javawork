package singleton.student;

public class School {
	private static School instance; 
	
	private School() {}
	
	public static School getInstance() {
		if(instance == null) {
			instance = new School();
		}
		return instance;
	}
	//학번 자동생성 메서드
	public Student studentNumber() {
		Student number = new Student();
		return number;
	}
	
}
