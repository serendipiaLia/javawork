package practice;

public class boolean1 {
	public static void main(String[] args) {
		
		int num1 = 10;
		int num2 = 20;
		
		boolean flag = (num1 > 0) && (num2 > 0);	
		System.out.println(flag); 	// T && T 이므로 결과 값 true
		
		flag = (num1 < 0) && (num2 > 0);	
		System.out.println(flag); // F && T 한개의 항만 T이므로 결과값 false
		
		flag = (num1 < 0) || (num2 > 0);
		System.out.println(flag);
		
	}
}


	
	
	
	
