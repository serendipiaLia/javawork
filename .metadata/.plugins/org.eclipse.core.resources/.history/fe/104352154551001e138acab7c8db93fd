package inheritance.q4;

public class Employee {

	public String name;
	public String grade;
	
	public Employee(String name, String grade) {
		this.name = name;
		this.grade = grade; //
	}
}

	private String skilset;
	public String getSkillSet() {
		return skillset;
	}
	public void setSkillSet(String skillset)
	this.skillSet = skillset;
