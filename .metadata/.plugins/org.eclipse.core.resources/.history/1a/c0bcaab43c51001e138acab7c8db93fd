package inheritance.statement;

public class SaleStatement {
	
	public static void main(String[] args) {
		//Drink 클래스 사용
		Drink coffee = new Drink("커피", 2500, 10);
		Drink greentea = new Drink("녹차", 3000, 4);
		
		//매출 전표 출력
		Drink.printTitle();		//제목행 
		coffee.printData();
		greentea.printData();
	}
}
